<div id="producers">
    <h2>Discover our producers</h2>
    <ul>
    {% for producer in producers %}
        <li>
            <a href="{{ path('open_miam_miam_producer', {'producer_slug': producer.slug}) }}">
                {{ producer }}
            </a>
        </li>
    {% endfor %}
    </ul>

    <p>
        <a href="#">See all producers</a>
    </p>
</div>